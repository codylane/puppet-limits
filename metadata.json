{
  "name": "puppet-limits",
  "version": "1.0.0",
  "author": "Cody Lane",
  "summary": "A module for managing security limits in pam",
  "license": "Apache-2.0",
  "source": "https://github.com/codylane/puppet-limits",
  "project_page": "https://github.com/codylane/puppet-limits",
